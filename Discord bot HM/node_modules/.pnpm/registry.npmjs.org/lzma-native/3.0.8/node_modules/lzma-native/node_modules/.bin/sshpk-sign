#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/jappie/Desktop/Discord bot HM/node_modules/.pnpm/registry.npmjs.org/lzma-native/3.0.8/node_modules/lzma-native/node_modules/sshpk/bin/sshpk-sign/node_modules:/Users/jappie/Desktop/Discord bot HM/node_modules/.pnpm/registry.npmjs.org/lzma-native/3.0.8/node_modules/lzma-native/node_modules/sshpk/bin/node_modules:/Users/jappie/Desktop/Discord bot HM/node_modules/.pnpm/registry.npmjs.org/lzma-native/3.0.8/node_modules/lzma-native/node_modules/sshpk/node_modules:/Users/jappie/Desktop/Discord bot HM/node_modules/.pnpm/registry.npmjs.org/lzma-native/3.0.8/node_modules/lzma-native/node_modules:/Users/jappie/Desktop/Discord bot HM/node_modules/.pnpm/registry.npmjs.org/lzma-native/3.0.8/node_modules:/Users/jappie/Desktop/Discord bot HM/node_modules/.pnpm/registry.npmjs.org/lzma-native/node_modules:/Users/jappie/Desktop/Discord bot HM/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/jappie/Desktop/Discord bot HM/node_modules/.pnpm/node_modules:/Users/jappie/Desktop/Discord bot HM/node_modules:/Users/jappie/Desktop/node_modules:/Users/jappie/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sshpk/bin/sshpk-sign" "$@"
else 
  exec node  "$basedir/../sshpk/bin/sshpk-sign" "$@"
fi
