#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/jappie/Desktop/Discord bot HM/node_modules/.pnpm/registry.npmjs.org/touch/3.1.0/node_modules/touch/bin/nodetouch.js/node_modules:/Users/jappie/Desktop/Discord bot HM/node_modules/.pnpm/registry.npmjs.org/touch/3.1.0/node_modules/touch/bin/node_modules:/Users/jappie/Desktop/Discord bot HM/node_modules/.pnpm/registry.npmjs.org/touch/3.1.0/node_modules/touch/node_modules:/Users/jappie/Desktop/Discord bot HM/node_modules/.pnpm/registry.npmjs.org/touch/3.1.0/node_modules:/Users/jappie/Desktop/Discord bot HM/node_modules/.pnpm/registry.npmjs.org/touch/node_modules:/Users/jappie/Desktop/Discord bot HM/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/jappie/Desktop/Discord bot HM/node_modules/.pnpm/node_modules:/Users/jappie/Desktop/Discord bot HM/node_modules:/Users/jappie/Desktop/node_modules:/Users/jappie/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../touch/3.1.0/node_modules/touch/bin/nodetouch.js" "$@"
else 
  exec node  "$basedir/../../../../../../touch/3.1.0/node_modules/touch/bin/nodetouch.js" "$@"
fi
