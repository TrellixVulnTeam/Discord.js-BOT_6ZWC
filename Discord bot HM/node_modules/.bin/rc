#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/jappie/Desktop/Discord bot HM/node_modules/rc/cli.js/node_modules:/Users/jappie/Desktop/Discord bot HM/node_modules/rc/node_modules:/Users/jappie/Desktop/Discord bot HM/node_modules:/Users/jappie/Desktop/node_modules:/Users/jappie/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rc/cli.js" "$@"
else 
  exec node  "$basedir/../rc/cli.js" "$@"
fi
